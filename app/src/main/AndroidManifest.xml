        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrdNewActivity"></activity>
        <activity android:name=".AdminListActivity" />
        <activity android:name=".StockChangeActivity" />
        <activity android:name=".PrdInfoActivity" />
        <activity android:name=".PrdListActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ScanCodeActivity" />
